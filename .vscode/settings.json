{
  "cSpell.words": ["Vitesse"],
  "prettier.enable": false,
  "typescript.tsdk": "node_modules/typescript/lib",
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true,
  },
  "files.associations": {
    "*.css": "postcss",
  },
  "i18n-ally.localesPaths": [
    "locales",
    "node_modules.bak/eslint/messages",
    "node_modules.bak/yargs/locales",
    "node_modules.bak/highlight.js/lib/languages",
    "node_modules.bak/workbox-core/models/messages",
    "node_modules.bak/workbox-core/src/models/messages"
  ],
  "eslint.alwaysShowStatus": true,
  "eslint.codeActionsOnSave.mode": "problems",
  "eslint.format.enable": true,
  "workbench.colorCustomizations": {
    "activityBar.activeBackground": "#abcbb2",
    "activityBar.activeBorder": "#8175aa",
    "activityBar.background": "#abcbb2",
    "activityBar.foreground": "#15202b",
    "activityBar.inactiveForeground": "#15202b99",
    "activityBarBadge.background": "#8175aa",
    "activityBarBadge.foreground": "#e7e7e7",
    "editorGroup.border": "#abcbb2",
    "panel.border": "#abcbb2",
    "sash.hoverBorder": "#abcbb2",
    "sideBar.border": "#abcbb2",
    "statusBar.background": "#8cb795",
    "statusBar.foreground": "#15202b",
    "statusBarItem.hoverBackground": "#6da378",
    "statusBarItem.remoteBackground": "#8cb795",
    "statusBarItem.remoteForeground": "#15202b",
    "titleBar.activeBackground": "#8cb795",
    "titleBar.activeForeground": "#15202b",
    "titleBar.inactiveBackground": "#8cb79599",
    "titleBar.inactiveForeground": "#15202b99"
  },
  "peacock.color": "#8cb795"
}
